<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY example "http://example.org#" >
]>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:example="http://example.org#">

    <!-- Classes -->
    <owl:Class rdf:about="&example;DecompositionOperation"/>
    <owl:Class rdf:about="&example;MappingOperation"/>
    <owl:Class rdf:about="&example;Variable"/>

    <!-- Individuals: Decomposition Templates -->
    <owl:NamedIndividual rdf:about="&example;DateDecompositionTemplate">
        <rdf:type rdf:resource="&example;DecompositionOperation"/>
        <example:hasName rdf:datatype="&xsd;string">Day</example:hasName>
        <example:hasDecompositionTemplate rdf:datatype="&xsd;string">(?P&lt;Year&gt;\d+)-(?P&lt;Month&gt;\d+)-(?P&lt;Day&gt;\d+)</example:hasDecompositionTemplate>
    </owl:NamedIndividual>

    <owl:NamedIndividual rdf:about="&example;NameDecompositionTemplate">
        <rdf:type rdf:resource="&example;DecompositionOperation"/>
        <example:hasName rdf:datatype="&xsd;string">LastName</example:hasName>
        <example:hasDecompositionTemplate rdf:datatype="&xsd;string">(?P&lt;FirstName&gt;[^_]+)_(?P&lt;LastName&gt;[^_]+)</example:hasDecompositionTemplate>
    </owl:NamedIndividual>
    

    

    <!-- Individuals: Mapping Templates -->
    <rdf:Description rdf:about="&example;SeasonMappingTemplate">
    <rdf:type rdf:resource="&example;MappingOperation"/>
    <example:hasName rdf:datatype="&xsd;string">Season</example:hasName>
    
    <example:hasMapping>
        <rdf:Description rdf:about="&example;Winter">
            <rdf:type rdf:resource="&example;MappingInterval"/>
            <example:hasMappingInterval rdf:datatype="&xsd;string">12-21/03-20</example:hasMappingInterval>
        </rdf:Description>
    </example:hasMapping>
    
    <example:hasMapping>
        <rdf:Description rdf:about="&example;Spring">
            <rdf:type rdf:resource="&example;MappingInterval"/>
            <example:hasMappingInterval rdf:datatype="&xsd;string">03-21/06-20</example:hasMappingInterval>
        </rdf:Description>
    </example:hasMapping>
    
    <example:hasMapping>
        <rdf:Description rdf:about="&example;Summer">
            <rdf:type rdf:resource="&example;MappingInterval"/>
            <example:hasMappingInterval rdf:datatype="&xsd;string">06-21/09-22</example:hasMappingInterval>
        </rdf:Description>
    </example:hasMapping>
    
    <example:hasMapping>
        <rdf:Description rdf:about="&example;Autumn">
            <rdf:type rdf:resource="&example;MappingInterval"/>
            <example:hasMappingInterval rdf:datatype="&xsd;string">09-23/12-20</example:hasMappingInterval>
        </rdf:Description>
    </example:hasMapping>
    </rdf:Description>


    <!-- Individuals: Variables -->
    <owl:NamedIndividual rdf:about="&example;Date">
        <rdf:type rdf:resource="&example;Variable"/>
        <example:hasOperation rdf:resource="&example;DateDecompositionTemplate"/>
    </owl:NamedIndividual>

</rdf:RDF>
